  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
   use_frameworks!
  
  # Pods for PlacePickerQ
    pod 'PlacePickerQ'
    pod 'SDWebImage/WebP','~> 4.4.7', :modular_headers => true
  
